<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controllers.user.WelcomeController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox alignment="CENTER" spacing="20.0" layoutX="100" layoutY="100" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="80.0">
        <Label fx:id="welcomeLabel" text="Welcome!" >
            <font>
                <Font size="24.0" />
            </font>
        </Label>

        <Button text="Go to User List" onAction="#goToUserList"/>
        <Button text="Logout" onAction="#handleLogout" />
        <Button text="View Profile" onAction="#goToProfile" />

    </VBox>
</AnchorPane>
